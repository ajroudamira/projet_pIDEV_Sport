<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AffichageTypeCoursController">
   <children>
      <TableView fx:id="tv_type" layoutX="104.0" layoutY="42.0" prefHeight="200.0" prefWidth="391.0">
        <columns>
          <TableColumn fx:id="col_nom" prefWidth="75.0" text="nom" />
          <TableColumn fx:id="col_objective" prefWidth="75.0" text="objective" />
            <TableColumn fx:id="col_description" prefWidth="75.0" text="description" />
            <TableColumn fx:id="col_calories" onEditCommit="#changecaloriesCellEvent" prefWidth="75.0" text="calories" />
        </columns>
      </TableView>
      <Button layoutX="118.0" layoutY="253.0" mnemonicParsing="false" onAction="#SelectType" text="selectionner" />
      <Button layoutX="232.0" layoutY="253.0" mnemonicParsing="false" onAction="#ModifierType" text="modifier" />
      <Button layoutX="342.0" layoutY="253.0" mnemonicParsing="false" onAction="#SupprimerType" text="supprimer" />
      <TextField fx:id="tf_newcalories" layoutX="219.0" layoutY="307.0" />
      <Label layoutX="130.0" layoutY="311.0" text="calories" />
   </children>
</AnchorPane>
